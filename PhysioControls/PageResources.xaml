<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ViewModel="clr-namespace:PhysioControls.ViewModel"
                    xmlns:View="clr-namespace:PhysioControls.View">
    <DataTemplate DataType="{x:Type ViewModel:NodeViewModel}">
        <View:NodeView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModel:SubNodeViewModel}">
        <View:SubNodeView/>
    </DataTemplate>
</ResourceDictionary>